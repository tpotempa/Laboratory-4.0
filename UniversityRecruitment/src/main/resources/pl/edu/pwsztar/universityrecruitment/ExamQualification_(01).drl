package pl.edu.pwsztar.universityrecruitment;
 
rule "Qualification #1-01"
ruleflow-group "admission-01"
when
    $uc : UniversityCandidate(study.equals("IN"), examResult >= 100)
then
    $uc.setAdmission(true);
    $uc.setQualificationType("Exam qualification!");
    $uc.appendLogger("Qualification #1 fired!");
end

